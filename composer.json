{
    "name": "bilal-azam/laravel-advanced-rbac",
    "description": "A Laravel package providing automated Role-Based Access Control (RBAC) with advanced permissions management.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bilal Azam",
            "email": "bilal.azam1@outlook.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/support": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Vendor\\AdvancedRBAC\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vendor\\AdvancedRBAC\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vendor\\AdvancedRBAC\\RBACServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0",
        "phpunit/phpunit": "^9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
